{
  "name": "pothole_detection",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "pre:commit": "yarn build && yarn lint --fix",
    "pm2:dev": "pm2 start yarn --name 'pothole web' --interpreter bash -- start -p 8022 && yarn pm2:logs",
    "pm2:prod": "pm2 start yarn --name 'pothole web' --interpreter bash -- start -p 8021 && yarn pm2:logs",
    "pm2:logs": "pm2 logs -n app-name --raw --merge-logs"
  },
