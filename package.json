    "moment": "^2.29.4",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-map-gl": "^7.0.19",
    "react-moment": "^1.1.2",
    "react-redux": "^8.0.4",
    "react-tooltip": "^4.4.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^4.0.0",
